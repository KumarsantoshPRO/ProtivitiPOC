<mvc:View
		controllerName="com.zmaruti.controller.App"
		height="100%"
		xmlns:mvc="sap.ui.core.mvc"
        xmlns:u="sap.ui.unified"

		xmlns:core="sap.ui.core"
		xmlns="sap.m"
		xmlns:f="sap.f"
		xmlns:fb="sap.ui.comp.filterbar"
		xmlns:svm="sap.ui.comp.smartvariants">
		<f:DynamicPage  headerExpanded="true" fitContent="true" showFooter="true">
		<f:title>
<f:DynamicPageTitle>
<f:heading>
<Title text="Admin-Part list upload Screen"/>
</f:heading>
</f:DynamicPageTitle>
</f:title>
			<f:header>
				<f:DynamicPageHeader>
					<f:content>
						  <OverflowToolbar>
        <u:FileUploader
                                    id="idFileUploader"
                                    name="myFileUpload"
                                    uploadUrl="upload/"
                                    tooltip="Upload Excel file to the server"
                                    uploadComplete="handleUploadComplete"
                                    change="handleUploadFile"
                                    typeMissmatch="handleTypeMissmatch"
                                    style="Ghost"
                                    multiple="true"
                                    buttonOnly="false" 
                                    iconOnly="false"
                                    icon="sap-icon://upload-to-cloud"
                                    fileType="XLSX,xls,csv"
                                    placeholder="Choose a file for Upload...">
            <u:parameters>
                <u:FileUploaderParameter
                                                name="Accept-CH"
                                                value="Viewport-Width"/>
                <u:FileUploaderParameter
                                                name="Accept-CH"
                                                value="Width"/>
                <u:FileUploaderParameter
                                                name="Accept-CH-Lifetime"
                                                value="86400"/>
            </u:parameters>
            
        </u:FileUploader>
        <ToolbarSpacer />
        <Button text="Add New Line" press="onAddPress"
            class="sapUiSmallMarginTopBottom" icon="sap-icon://add">

            
                    <layoutData>
                    <OverflowToolbarLayoutData priority="Low" />
                </layoutData>
                </Button>
                 <!-- <Button 
                text="Delete"
                icon="sap-icon://delete"
                press="onDeletePress"/> -->
         </OverflowToolbar>
   
					</f:content>
				</f:DynamicPageHeader>
			</f:header>
			<f:content>
			   <ScrollContainer  width="100%" vertical="true" focusable="true">
        <Table width="100%" items="{excelData>/}">
            <columns>
                <!-- <Column hAlign="Center" width="5rem" >
                    <Text text="Select" />
                </Column> -->
                <Column hAlign="Center" width="10rem">
                    <Text text="Part No" />
                </Column>
                <Column hAlign="Center" width="10rem">
                    <Text text="Part Name" />
                </Column>
                <Column hAlign="Center" width="10rem">
                    <Text text="Part Category" />
                </Column>
                <Column hAlign="Center" width="10rem">
                    <Text text="Part Family" />
                </Column>
                <Column hAlign="Center" width="10rem">
                    <Text text="Model" />
                </Column>
                <Column hAlign="Center" width="10rem">
                    <Text text="Engine" />
                </Column>
                <Column hAlign="Center" width="10rem">
                    <Text text="Vendor Code" />
                </Column>
                <Column hAlign="Center" width="10rem">
                    <Text text="Vendor Name" />
                </Column>
                <Column hAlign="Center" width="10rem">
                    <Text text="Department" />
                </Column>
                <Column hAlign="Center" width="10rem">
                    <Text text="PIC User Id - Name" />
                </Column>
                <Column hAlign="Center" width="10rem">
                    <Text text="Section mgr user id - Name" />
                </Column>
                <Column hAlign="Center" width="10rem">
                    <Text text="Other applicable Model/Engine" />
                </Column>
                <Column hAlign="Center" width="10rem">
                    <Text text="Batch Code availability (Y/N)" />
                </Column>
                <Column hAlign="Center" width="10rem">
                    <Text text="Remarks(if any)" />
                </Column>
                
            </columns>
            <items>
                <ColumnListItem vAlign="Middle">
                    
                    <cells>
                        <!-- <CheckBox selected="{excelData>selected}" /> -->
                        <Input value="{excelData>PartNo}" />
                        <Input value="{excelData>PartName}" />
                        <ComboBox selectedKey="{excelData>PartCategory}">
                                    <items>
                                        <core:Item key="A" text="A" />
                                        <core:Item key="B" text="B" />
                                        <core:Item key="C" text="C" />
                                    </items>
                                </ComboBox>
                        <ComboBox selectedKey="{excelData>PartFamily}">
                                    <items>
                                        <core:Item key="Hose Pipe" text="Hose Pipe" />	
                                        <core:Item key="Pipe Hose" text="Pipe Hose" />
                                        <core:Item key="Pipe Brake" text="Pipe Brake" />
                                        <core:Item key="Hose" text="Hose" />

                                    </items>
                                </ComboBox>
                        <Input value="{excelData>Model}" />
                        <Input value="{excelData>Engine}" />
                        <Input value="{excelData>VendorCode}" />
                        <Input value="{excelData>VendorName}" />
                        <Input value="{excelData>Department}" />
                        <Input value="{excelData>UserId}" />
                        <Input value="{excelData>ManagerName}" />
                        <Input value="{excelData>OtherEngine}" />
                        <ComboBox selectedKey="{excelData>BatchAvailability}">
                                    <items>
                                        <core:Item key="Y" text="Yes" />
                                        <core:Item key="N" text="No" />
                                        
                                       
                                    </items>
                                </ComboBox>
                        <Input value="{excelData>Remarks}" />
                        
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
        </ScrollContainer>
        						

			</f:content>
            <f:footer>
            <OverflowToolbar>
                                
<ToolbarSpacer/>
<Button type="Accept" text="Inquiry"/>
<Button type="Emphasized" icon="sap-icon://save" text="Save" press="onSendData"/>

        						</OverflowToolbar>
                                </f:footer>
		</f:DynamicPage>

        <!-- </Page> -->
</mvc:View>
