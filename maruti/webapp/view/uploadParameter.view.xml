<mvc:View
    controllerName="misp.maruti.controller.PartMaster.Upload"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    displayBlock="true"
    height="100%"
    busyIndicatorDelay="0"
    xmlns:form="sap.ui.layout.form"
    xmlns:f="sap.f"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:svm="sap.ui.comp.smartvariants"
    xmlns:tnt="sap.tnt"
>
    <Page
        title="Upload Parameters"
        titleAlignment="Center"
    >
        <f:DynamicPage
            headerExpanded="{/headerExpanded}"
            showFooter="true"
        >
            <f:header>
                <f:DynamicPageHeader>
                    <f:content>
                        <fb:FilterBar
                            id="filterbar"
                            persistencyKey="myPersKey"
                            useToolbar="false"
                            search=".onSearch"
                            filterChange=".onFilterChange"
                            afterVariantLoad=".onAfterVariantLoad"
                        >
                            <fb:filterGroupItems>
                                <fb:FilterGroupItem
                                    name="Request_Number"
                                    label="Request Number"
                                    groupName="Group1"
                                    visibleInFilterBar="true"
                                >
                                    <fb:control>
                                        <Input />
                                    </fb:control>
                                </fb:FilterGroupItem>

                                <fb:FilterGroupItem
                                    name="Control_No"
                                    label="Control Number"
                                    groupName="Group1"
                                    visibleInFilterBar="true"
                                >
                                    <fb:control>
                                        <Input />
                                    </fb:control>
                                </fb:FilterGroupItem>
                            </fb:filterGroupItems>
                        </fb:FilterBar>
                    </f:content>
                </f:DynamicPageHeader>
            </f:header>
            <f:content>
                <HBox alignContent="Center" alignItems="Center">
                    <VBox>
                        <tnt:NavigationList
                            id="navigationList"
                            width="320px"
                            selectedKey="subItem3"
                            
                        >
                            <tnt:NavigationListItem
                                text="Section 1"
                                key="rootItem1"
                            >
                                <tnt:NavigationListItem
                                    text="Appearance"
                                    key="subItem3"
                                />
                                <tnt:NavigationListItem text="Dimentions" />
                                <tnt:NavigationListItem text="Material" />
                                <tnt:NavigationListItem text="Performance" />
                                <tnt:NavigationListItem text="Regulatory" />
                            </tnt:NavigationListItem>
                        </tnt:NavigationList>
                        <Button
                            text="Upload"
                            width="300px"
                            press="onUploadSection1"
                        />
                    </VBox>
                    <VBox>
                        <tnt:NavigationList
                          
                            width="320px"
                            selectedKey="subItem3"
                        >
                            <tnt:NavigationListItem
                                text="Section 2"
                                key="rootItem1"
                            >
                                <tnt:NavigationListItem
                                    text="Dimension Photos"
                                    key="subItem3"
                                />
                                <tnt:NavigationListItem text="BOM" />
                                <tnt:NavigationListItem text="Regulatory Marking" />
                                <tnt:NavigationListItem text="Traceability Details" />
                                <tnt:NavigationListItem text="Additional Annexures" />
                            </tnt:NavigationListItem>
                        </tnt:NavigationList>
                        <Button
                            text="Upload"
                            width="300px"
                            press="onUploadSection2"
                        />
                    </VBox>
                </HBox>
            </f:content>
            <!-- DynamicPage Footer -->
            
        </f:DynamicPage>
    </Page>
</mvc:View>
